<h1>Zone list</h1>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Updated</th>
      <th>Version</th>
    </tr>
  </thead>
  <tbody>
  <% @zones.each do |zone| %>
    <tr>
      <td><%= link_to zone['name'], zone_show_path(zone['id']) %></td>
      <td><%= format_date(zone['date_updated']) %></td>
      <td><%= zone['version'] %></td>
    </tr>
  <% end %>
</tbody>
</table>
